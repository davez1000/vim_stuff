{
  "require": {
    "tpope/vim-pathogen": "~2.4",
    "junegunn/fzf.vim": "dev-master",
    "junegunn/fzf": "~0.16.9",
    "tpope/vim-fugitive": "~2.2",
    "easymotion/vim-easymotion": "~3.0",
    "scrooloose/nerdtree": "~5.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:tpope/vim-pathogen.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:junegunn/fzf.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/junegunn/fzf.vim.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:tpope/vim-fugitive.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:easymotion/vim-easymotion.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:scrooloose/nerdtree.git"
    }
  ],
  "extra": {
    "installer-paths": {
      ".vim/autoload/": ["tpope/vim-pathogen"],
      ".vim/bundle/": ["junegunn/fzf.vim"],
      ".vim/bundle/": ["junegunn/fzf"],
      ".vim/bundle/": ["tpope/vim-fugitive"],
      ".vim/bundle/": ["easymotion/vim-easymotion"],
      ".vim/bundle/": ["scrooloose/nerdtree"]
    }
  }
}
